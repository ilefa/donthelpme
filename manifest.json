{
    "name": "donthelpme",
    "description": "Removes the help button in Mimir that overlaps the test cases view.",
    "manifest_version": 2,
    "version": "0.0.1",
    "homepage_url": "https://m1ke.co",
    "icons": {
        "48": "icons/donthelpme.png"
    },
    "content_scripts": [
        {
            "matches": ["*://class.mimir.io/*"],
            "js": ["donthelpme.js"]
        }
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "donthelpme@m1ke.co"
        }
    }
}